@using FrontendAccountCreation.Web.Constants
@using FrontendAccountCreation.Web.ViewModels.ReExAccount
@model ApprovedPersonViewModel
@{
	ViewData["Title"] = Localizer["YouAreApprovedPerson.Title"];
	ViewData["ApplicationTitleOverride"] = LayoutOverrides.ReExTitleOverride;
	ViewData["HeaderOverride"] = LayoutOverrides.ReExOrganisationHeaderOverride;
}

@if (Model.IsLimitedLiabilityPartnership)
{
	@Html.Partial("LimitedLiabilityPartnershipApprovedPerson")
}
else if (Model.IsLimitedPartnership)
{
	@Html.Partial("LimitedPartnershipYouAreApprovedPerson")
}
else
{
	@Html.Partial("NonPartnershipApprovedPerson")
}

